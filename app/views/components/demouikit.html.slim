.tasa-div-form
  .section-header
    |Titulo de seccion
  .section-body
    form
      .col-lg-6.container-fluid
        .tasa-input  [formGroup]="registerForm"
          label for="firstname" Label de un input
          tasa-tooltip [text]= "'Texto desde UI'" [position]= "'top'"
          input placeholder="Agrega un valor" formControlName="firstname" [(ngModel)] ="model.name"
        .tasa-input
          label for="inputdemo2" [ngClass]="{'label-disabled': disabledLabel}"
            |Label de un input desabilitado
          input name="inputdemo2" [disabled]='disabledLabel'
          .error-message Error de validacion
        .tasa-input
          label for='selectdemo' Label para select
          .tasa-select
            select.option-deafult name="selectdemo" [(ngModel)]='combo' [ngClass]="{'selectedOption':combo!=''}"
              option value="" Seleciona
              option template="ngFor let opcion of opciones" [value]="opcion.id" {{opcion.texto}}
      .col-lg-6.container-fluid
        .tasa-input
          label for="inputdemo1" Label de un input 
          input name="inputdemo1" placeholder="Agrega un valor"
        .tasa-input
          label for="inputdemo2" [ngClass]="{'label-disabled': disabledLabel}"
            |Label de un input desabilitado
          input name="inputdemo2" [disabled]='disabledLabel'
        .tasa-input
          label for='selectdemo' [ngClass]="{'label-disabled': disabledLabel}" Label para select
          .tasa-select
            select.option-deafult name="selectdemo" [(ngModel)]='combo' [ngClass]="{'selectedOption':combo!=''}" [disabled]='disabledLabel'
              option value="" Seleciona
              option template="ngFor let opcion of opciones" [value]="opcion.id" {{opcion.texto}}
