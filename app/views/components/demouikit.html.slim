  .row.radio-group  [ngSwitch]="imagen"
    .imagen-btn
      img src="{{imagen == 0 ? '/images/RviajeOn-01.svg' : '/images/RviajeOff-01-01.svg'}}"
      br
      button.btn-image.waves-effect.waves-button.waves-float.waves-light [ngClass]="{'active': imagen === '0'}" [disabled]='disabledLabel' [ngSwitch]="imagen"
        div
          span.button-text-icon Texto de boton1
    .imagen-btn
      img src="{{imagen == 1 ? '/images/RviajeOn-01.svg' : '/images/RviajeOff-01-01.svg'}}"
      br
      button.btn-image.waves-effect.waves-button.waves-float.waves-light [ngClass]="{'active': imagen === 1}" [ngSwitch]="imagen" (click)="imagen = 1"
        div
          span.button-text-icon Texto de boton2
    .imagen-btn
      img src="{{imagen == 2 ? '/images/RviajeOn-01.svg' : '/images/RviajeOff-01-01.svg'}}"
      br
      button.btn-image.waves-effect.waves-button.waves-float.waves-light [ngClass]="{'active': imagen === 2}" [ngSwitch]="imagen" (click)="imagen = 2"
        div 
          span.button-text-icon Texto de boton3
    .imagen-btn
      button.btn-image.waves-effect.waves-button.waves-float.waves-light [ngClass]="{'active': imagen === 3}" [ngSwitch]="imagen" (click)="imagen = 3"
        div 
          span.button-text-icon boton sin imagen
  br
